<%= form_for @dish do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :region %>
    <%= f.text_field :region %>

    <%= f.label :course %>
    <%= f.text_field :course %>

    <%= f.label :main_ingredient %>
    <%= f.text_field :main_ingredient %>

    <%= f.label :image %>
    <%= f.url_field :image %>

    <%= f.submit %>
<% end %>