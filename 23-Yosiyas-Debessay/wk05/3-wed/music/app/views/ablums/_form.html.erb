<%= form_for @album do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    
    <%= f.label :date %>
    <%= f.text_field :date %>

    <%= f.label :not %>
    <%= f.text_field :not %>

    <%= f.label :genre %>
    <%= f.text_field :genre %>   

    <%= f.label :image %>
    <%= f.url_field :image %> 

    <%= f.submit %>
<% end %>