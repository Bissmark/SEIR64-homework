<h1>
    <%= @car["make"] %>
</h1>

<h2>
    Model: <%= @car["model"] %>
</h2>

<h3>
    Series: <%= @car["series"] %>
</h3>

<h4>
    Year: <%= @car["year"] %>
</h4>

<% unless @car["image"].nil? || @car["image"].empty? %>
    <img class="feature" src="<%= @car["image"] %>" alt="<%= @car["name"] %>">
<% end %>

<div class="controls">
    <a href="/cars/<%= @car["id"] %>/edit">Edit Car</a>
    <a href="/cars/<%= @car["id"] %>/delete">Delete Car</a>
</div>