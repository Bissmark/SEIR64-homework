<%= form_for @album do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :released %>
    <%= f.text_field :released %>

    <%= f.label :genre %>
    <%= f.text_field :genre %>

    <%= f.label :artist_id %>
    <%= f.text_field :artist_id %>

    <%= f.label :image %>
    <%= f.url_field :image %>

    <%= f.submit %>
    
<% end %>