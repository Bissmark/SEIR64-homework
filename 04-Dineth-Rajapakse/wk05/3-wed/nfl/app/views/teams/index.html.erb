<h1>All Teams</h1>


<% @teams.each do |team| %>
    <%= image_tag team.image, :class => 'thumb' if team.image.present? %>
    <ul>
        <li>Name: <%= link_to team.name, team %></li>
        <li>Owner: <%= team.owner %></li>
        <li>Superbowls: <%= team.championships %></li>
     
    </ul>
<% end %>
