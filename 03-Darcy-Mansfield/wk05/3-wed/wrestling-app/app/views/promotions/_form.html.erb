<%= form_for @promotion do |f| %> 

    <%= f.label :promotion %>
    <%= f.text_field :promotion %>

    <%= f.label :logo %>
    <%= f.url_field :logo %>

    <%= f.label :country %>
    <%= f.text_field :country %>

    <%= f.label :year %>
    <%= f.number_field :year %>

    <%= f.label :instagram %>
    <%= f.url_field :instagram %>

    <%= f.submit %>

<% end %>